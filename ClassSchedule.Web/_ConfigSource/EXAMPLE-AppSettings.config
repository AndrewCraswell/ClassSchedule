<appSettings>
  <add key="ClientValidationEnabled" value="true" />
  <add key="UnobtrusiveJavaScriptEnabled" value="true" />
  <add key="elmah.mvc.disableHandler" value="false" />
  <add key="elmah.mvc.disableHandleErrorFilter" value="false" />
  <add key="elmah.mvc.requiresAuthentication" value="true" />
  <add key="elmah.mvc.allowedRoles" value="" /> <!-- ASP.NET Role to grant access to log -->
  <add key="elmah.mvc.route" value="elmah" />
<!-- CTC Class Schedule Settings -->
  <add key="currentSchoolUrl" value="" />
  <add key="courseOutcomesUrl" value=""/>
  <add key="collegeCode" value=""/>
  <add key="collegeName" value=""/>
  <add key="IncludeCourseOutcomes" value="true" />
<!-- CTC Class Schedule roles: A comma-separated list of Active Directory groups ("Roles" in ASP.NET) -->
  <add key="ApplicationDeveloper" value="" />
  <add key="ApplicationAdmin" value="" />
  <add key="ApplicationEditor" value="" />
<!-- CTC Class Schedule Caching settings for views in seconds -->
  <add key="IndexCacheTime" value="0"/>
  <add key="AllClassesCacheTime" value="0"/>
  <add key="SubjectCacheTime" value="0"/>
  <add key="YearQuarterCacheTime" value="0"/>
  <add key="YearQuarterSubjectCacheTime" value="0"/>
  <add key="ClassDetailsCacheTime" value="0"/>
<!-- CTC ODS API settings -->
  <add key="CourseDescriptionHandler" value="" /> <!-- Fully-qualified class name (w/ namespace) which impelements https://github.com/BellevueCollege/CtcApi/blob/dev/CtcApi.Ods/Customizations/ICourseDescriptionStrategy.cs -->
  <add key="CurrentDateOverride" value=""/>
</appSettings>
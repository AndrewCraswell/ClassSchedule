<div id="a_to_z">

@{
	String letter = ViewBag.letter;
	String YearQuarter = ViewBag.YearQuarter;
	String time = ViewBag.time;
	String days = ViewBag.days;
	String flex = ViewBag.flex;
	String avail = ViewBag.avail;


	// construct the route values we'll pass to the ActionLink helper
	IDictionary<string, object> routeParms = ViewBag.LinkParams;
	if (!string.IsNullOrWhiteSpace(ViewBag.letter))
	{
		routeParms.Add("letter", ViewBag.letter);
	}

	string Controller = ViewContext.RouteData.Values["Controller"].ToString();
	string Action = ViewContext.RouteData.Values["Action"].ToString();



}

@* print out the ascii values for a-z  *@
@foreach (string let in ViewBag.Alphabet)
{
	routeParms.Remove("letter");
	routeParms.Add("letter", let.ToString());

	@Html.ActionLink(let.ToString(), Controller, Action, new RouteValueDictionary(routeParms), null) <text>| </text>
	@*Html.ActionLink(let.ToString(), @YearQuarter, new { time = time, flex = flex, days = days, avail = avail, type = "beta", letter = let.ToString() }) <text>| </text> *@
}

@Html.ActionLink("View All", @YearQuarter, new { time = time, flex = flex, days = days, avail = avail, type = "beta", letter = "" })
</div>


